<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <sectionGroup name="codebuilder">
      <section name="settings" type="CodeBuilder.Configuration.SettingsSection, CodeBuilder.Framework"/>
      <section name="typeMapping" type="CodeBuilder.Configuration.TypeMappingSection, CodeBuilder.Framework" />
    </sectionGroup>
  </configSections>
  <codebuilder>
    <settings></settings>
    <!--<typeMapping configSource="typeMapping.config">-->
    <typeMapping>
      <mappings>
        <mapping name="sqlserver2005-csharp" database="sqlserver2005" language="csharp">
          <types>
            <add name="nvarchar" langtype="string" default="" />
          </types>
        </mapping>
        <mapping name="sqlserver2005-java" database="sqlserver2005" language="java">
          <types>
            <add name="nvarchar" langtype="string" default="" />
          </types>
        </mapping>
      </mappings>
    </typeMapping>
  </codebuilder>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>
</configuration>